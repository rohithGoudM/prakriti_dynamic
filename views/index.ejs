	<!-- upcoming -->


<section id="upcoming">

		<div class="container">
			<div class="row">
			<div class="col-lg-12 text-center">
			<h2 class="section-heading">Upcoming events</h2>

<% if(up_events){ %>
<%var i=0;up_events.forEach(function(event){ i++;%>
    <% if(i%2==1){ %>
             <h3 class="section-subheading text-muted"></h3>
             <div class="container">
             <div class="row">
                <div class="col-lg-6">
                    <img src="<%=event.picUrl%>" style="height: 100%; width: 100%; object-fit: cover; float:middle;margin-left: auto;margin-right: auto;" >

                 </div>

                 <div class="col-lg-6">
                     <h2><%=event.name%></h2>
                    <p style="float:middle; margin:10%;  text-align: justify;"><%=event.writeup%></p>
                 </div>
             </div>
            </div>
    <% }else{ %>
    
             <h3 class="section-subheading text-muted"></h3>

             <div class="container">

             <div class="row">
                 <div class="col-lg-6 col-md-push-6">
                    <img src="<%=event.picUrl%>" class="img-responsive" style="height: 100%; width: 100%; object-fit: cover; float:middle;margin-left: auto;margin-right: auto;" >

                 </div>
                 <div class="col-lg-6 col-md-pull-6">
                     <h2><%=event.name%></h2>
                    <p style="float:middle; margin:10%;text-align: justify;"><%=event.writeup%>
                    </p>
                 </div>

             </div>


            </div>

<% } }); %>
<% } %>


		
	</section> 


